function validateRegisterForm() {
        var username = document.getElementById('register-username-2').value;
        var email = document.getElementById('register-email-2').value;
        var number = document.getElementById('register-number-2').value;
        var password = document.getElementById('register-password-2').value;
        var confirmPassword = document.getElementById('register-password-3').value;

        if (username.trim() ===''||email.trim() ===''||password.trim() ===''||confirmPassword.trim()===''||number.trim()==='') {
            setSweetAlert({icon:"info",text:'All fields are required'})
            return false;
        }
        var emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
        if (!emailRegex.test(email)) {
            setSweetAlert({icon:"info",text:'Please enter a valid email address'})
            return false;
        }
        const phoneRegex = /^[0-9]{10}$/;
        if (!phoneRegex.test(number)) {
            setSweetAlert({icon:"info",text:'invalid phone number'})
            return false;
        }

        if (password !== confirmPassword) {
            setSweetAlert({icon:"info",text:'Passwords do not match'})
            return false;
        }
        return true;
    }
    function setSweetAlert(obj){
        console.log(obj)
        Swal.fire({
            icon: obj.icon,
            title:'Info',
            text:obj.text
        });
    }

    document.getElementById('register').addEventListener('submit', function(e) {
        if (!validateRegisterForm()) {
            e.preventDefault();
        }
    });




