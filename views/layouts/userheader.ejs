    <style>
        @media (max-width: 991px) {
            .header-left {
                display: flex;
                align-items: center;
                /* Ensure vertical alignment */
                justify-content: center;
                /* Center horizontally */
                width: 100%;
                /* Take full width to ensure true centering if header-right is hidden or small */
            }
        }

        @media (min-width: 992px) {
            .main-nav {
                margin-left: 12rem !important;
            }
        }

        .logo-text {
            font-size: large;
            font-weight: 900;
            color: black;
        }

        @media (max-width: 576px) {
            .logo-text {
                font-size: 1.6rem !important;
            }

            .logo img {
                width: 80px;
                height: auto;
            }
        }

        /* Fix horizontal scroll */
        .page-wrapper {
            overflow-x: hidden;
            width: 100%;
        }

        body {
            overflow-x: hidden;
        }

        /* Hamburger Menu Button - Mobile Only */
        .mobile-menu-btn {
            display: none;
            position: absolute;
            top: 28px;
            right: 20px;
            z-index: 50;
            background: transparent;
            border: none;
            width: 50px;
            height: 50px;
            cursor: pointer;
            transition: all 0.3s ease;
        }

        .mobile-menu-btn:hover {
            transform: scale(1.1);
        }

        .mobile-menu-btn i {
            color: #333;
            font-size: 24px;
            transition: color 0.3s ease;
        }

        .mobile-menu-btn:hover i {
            color: #c96;
        }

        @media (max-width: 991px) {
            .mobile-menu-btn {
                display: flex;
                align-items: center;
                justify-content: center;
            }
        }

        /* Fullscreen Modal */
        .mobile-menu-modal {
            display: none;
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.5);
            z-index: 1050;
            opacity: 0;
            transition: opacity 0.3s ease;
        }

        .mobile-menu-modal.active {
            display: block;
            opacity: 1;
        }

        .mobile-menu-content {
            position: fixed;
            top: 0;
            right: -100%;
            width: 80%;
            max-width: 400px;
            height: 100%;
            background: #fff;
            box-shadow: -5px 0 15px rgba(0, 0, 0, 0.3);
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            padding: 20px;
            transition: right 0.4s cubic-bezier(0.4, 0, 0.2, 1);
            overflow-y: auto;
        }

        .mobile-menu-modal.active .mobile-menu-content {
            right: 0;
        }

        .mobile-menu-close {
            position: absolute;
            top: 20px;
            left: 20px;
            background: transparent;
            border: none;
            color: #333;
            font-size: 32px;
            cursor: pointer;
            width: 40px;
            height: 40px;
            display: flex;
            align-items: center;
            justify-content: center;
            transition: all 0.3s ease;
            z-index: 10;
        }

        .mobile-menu-close:hover {
            transform: rotate(90deg);
            color: #c96;
        }

        .mobile-menu-list {
            list-style: none;
            padding: 0;
            margin: 0;
            text-align: center;
            width: 100%;
        }

        .mobile-menu-list li {
            margin: 15px 0;
            opacity: 0;
            transform: translateX(30px);
            transition: all 0.3s ease;
        }

        .mobile-menu-modal.active .mobile-menu-list li {
            opacity: 1;
            transform: translateX(0);
        }

        .mobile-menu-list li:nth-child(1) {
            transition-delay: 0.1s;
        }

        .mobile-menu-list li:nth-child(2) {
            transition-delay: 0.15s;
        }

        .mobile-menu-list li:nth-child(3) {
            transition-delay: 0.2s;
        }

        .mobile-menu-list li:nth-child(4) {
            transition-delay: 0.25s;
        }

        .mobile-menu-list li:nth-child(5) {
            transition-delay: 0.3s;
        }

        .mobile-menu-list li:nth-child(6) {
            transition-delay: 0.35s;
        }

        .mobile-menu-list li:nth-child(7) {
            transition-delay: 0.4s;
        }

        .mobile-menu-list a {
            color: #333;
            text-decoration: none;
            font-size: 20px;
            font-weight: 500;
            text-transform: uppercase;
            letter-spacing: 1px;
            transition: all 0.3s ease;
            display: block;
            padding: 12px 20px;
            position: relative;
            border-radius: 8px;
        }

        .mobile-menu-list a:hover {
            color: #fff;
            background: #c96;
            transform: translateX(-5px);
        }

        @media (max-width: 480px) {
            .mobile-menu-content {
                width: 85%;
            }
        }
    </style>
    <script src="https://kit.fontawesome.com/dbad7a6aaf.js" crossorigin="anonymous"></script>
</head>

